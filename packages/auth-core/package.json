{
  "name": "@chicory/auth-core",
  "version": "0.1.0",
  "description": "Core authentication for Chicory - local auth with email/password",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "uuid": "^9.0.0",
    "mongodb": "^6.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "@remix-run/node": "^2.0.0",
    "react": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "@remix-run/node": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "license": "MIT"
}
